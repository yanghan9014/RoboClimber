                                    <geom name="finger_tip_r1" fromto=".0 .02 -.02 .015 .035 -.005" size="0.014" type="capsule"/> 
                                    <geom name="finger_tip_r2" fromto=".0 .0 .0 .015 .015 .015" size="0.014" type="capsule"/> 
                                    <geom name="finger_tip_r3" fromto=".0 -.02 .02 .015 -.005 .035" size="0.014" type="capsule"/> 
                                    <site name="top_site_r" pos=".01 -.005 -.005" size="0.012" rgba="0 0 0 .2"/>
                                    <site name="tip_site_r" pos=".025 .01 .01" size="0.012" rgba="0 0 0 .2"/>
                                </body>
                            </body>
                        </body>
                    </body>
                    <camera pos="0 0 0"/>
                </body>
            </body>
            <body name="left_upper_arm" pos="0 0.17 0.06">
                <joint armature="0.0068" axis="2 -1 1" name="left_shoulder1" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
                <joint armature="0.0051" axis="0 1 1" name="left_shoulder2" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
                <joint armature="0.0051" axis="-1 -1 1" name="left_shoulder3" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
                <geom fromto="0 0 0 .16 .16 -.16" name="left_uarm1" size="0.04 0.16" type="capsule"/>
                <body name="left_lower_arm" pos=".18 .18 -.18">
                    <joint armature="0.0028" axis="0 -1 -1" name="left_elbow" pos="0 0 0" range="-90 50" stiffness="2" type="hinge"/>
                    <geom fromto="0.01 -0.01 0.01 .17 -.17 .17" name="left_larm" size="0.031" type="capsule"/>
                    <body name="left_hand" pos=".19 -.19 .19">
                        <joint armature="0.0028" axis="0 1 1" name="left_wrist" pos="0 0 0" range="-40 60" stiffness="2" type="hinge"/>
