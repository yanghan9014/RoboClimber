<mujoco model="humanoid">
    <compiler angle="degree" inertiafromgeom="true"/>
    <default>
        <joint armature="1" damping="1" limited="true"/>
        <geom conaffinity="1" condim="6" contype="1" margin="0.002" gap="0.002" material="geom" rgba="0.8 0.6 .4 1"  friction="2.0 .05 .05"/>
        <motor ctrllimited="true" ctrlrange="-.4 .4"/>
    </default>
    <option integrator="RK4" iterations="50" solver="PGS" timestep="0.003">
        <!-- <flags solverstat="enable" energy="enable"/>-->
    </option>
    <size nkey="5" nuser_geom="1"/>
    <visual>
        <map fogend="5" fogstart="3"/>
    </visual>
    <asset>
        <texture builtin="gradient" height="100" rgb1=".4 .5 .6" rgb2="0 0 0" type="skybox" width="100"/>
        <!-- <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>-->
        <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
        <material name="geom" texture="texgeom" texuniform="true"/>
        <mesh name="foot_mesh_right" file="foot_right.obj"/>
        <mesh name="foot_mesh_left" scale="1 -1 1" file="foot_right.obj"/>
        <!-- Non-convex mesh collision isn't supported -->
        <!-- <mesh name="hook_mesh_right" scale="1.5 0.5 2"file="hook.obj"/>
        <mesh name="hook_mesh_left" scale="1.5 0.5 2" file="hook.obj"/> -->
        <!-- <mesh name="ladder" file="ladder.obj"/> -->
    </asset>
    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
        <geom condim="3" friction="1 .1 .1" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="20 20 0.125" type="plane"/>
        <!-- <geom condim="3" material="MatPlane" name="floor" pos="0 0 0" size="10 10 0.125" type="plane"/>-->
        <body name="ladder" pos="0.4 0 0">
            <!-- <geom condim="3" friction="1 .1 .1" name="ladder" pos="0.3 0 0" type="mesh" mesh="ladder"/> -->
            <geom name="rung0" type="capsule" size="0.01 2" pos="-.1 0 .2" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung1" type="capsule" size="0.01 2" pos="-.05 0 .4" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung2" type="capsule" size="0.01 2" pos="0 0 .6" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung3" type="capsule" size="0.01 2" pos=".05 0 .8" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung4" type="capsule" size="0.01 2" pos=".1 0 1.0" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung5" type="capsule" size="0.01 2" pos=".15 0 1.2" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung6" type="capsule" size="0.01 2" pos=".2 0 1.4" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung7" type="capsule" size="0.01 2" pos=".25 0 1.6" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung8" type="capsule" size="0.01 2" pos=".3 0 1.8" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung9" type="capsule" size="0.01 2" pos=".35 0 2.0" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung10" type="capsule" size="0.01 2" pos=".4 0 2.2" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung11" type="capsule" size="0.01 2" pos=".45 0 2.4" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung12" type="capsule" size="0.01 2" pos=".5 0 2.6" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung13" type="capsule" size="0.01 2" pos=".55 0 2.8" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
            <geom name="rung14" type="capsule" size="0.01 2" pos=".6 0 3.0" axisangle="1 0 0 90" rgba="0.8 0.4 0 1"/>
        </body>
        <body name="torso" pos="0 0 1.35">
            <camera name="track" mode="trackcom" pos="0 -4 0" xyaxes="1 0 0 0 0 1"/>
            <joint armature="0" damping="0" limited="false" name="root" pos="0 0 0" stiffness="0" type="free"/>
            <geom fromto="0 -.07 0 0 .07 0" name="torso1" size="0.07" type="capsule"/>
            <geom name="head" pos="0 0 .19" size=".09" type="sphere" user="258"/>
            <geom fromto="-.01 -.06 -.12 -.01 .06 -.12" name="uwaist" size="0.06" type="capsule"/>
            <body name="lwaist" pos="-.01 0 -0.260" quat="1.000 0 -0.002 0">
                <geom fromto="0 -.06 0 0 .06 0" name="lwaist" size="0.06" type="capsule"/>
                <joint armature="0.02" axis="0 0 1" damping="5" name="abdomen_z" pos="0 0 0.065" range="-45 45" stiffness="20" type="hinge"/>
                <joint armature="0.02" axis="0 1 0" damping="5" name="abdomen_y" pos="0 0 0.065" range="-75 30" stiffness="10" type="hinge"/>
                <body name="pelvis" pos="0 0 -0.165" quat="1.000 0 -0.002 0">
                    <joint armature="0.02" axis="1 0 0" damping="5" name="abdomen_x" pos="0 0 0.1" range="-35 35" stiffness="10" type="hinge"/>
                    <geom fromto="-.02 -.07 0 -.02 .07 0" name="butt" size="0.09" type="capsule"/>
                    <body name="right_thigh" pos="0 -0.1 -0.04">
                        <joint armature="0.01" axis="1 0 0" damping="5" name="right_hip_x" pos="0 0 0" range="-25 5" stiffness="10" type="hinge"/>
                        <joint armature="0.01" axis="0 0 1" damping="5" name="right_hip_z" pos="0 0 0" range="-60 35" stiffness="10" type="hinge"/>
                        <joint armature="0.0080" axis="0 1 0" damping="5" name="right_hip_y" pos="0 0 0" range="-110 20" stiffness="20" type="hinge"/>
                        <geom fromto="0 0 0 0 0.01 -.34" name="right_thigh1" size="0.06" type="capsule"/>
                        <body name="right_shin" pos="0 0.01 -0.403">
                            <joint armature="0.0060" axis="0 -1 0" name="right_knee" pos="0 0 .02" range="-160 -2"  stiffness="10" type="hinge"/>
                            <geom fromto="0 0 0 0 0 -.3" name="right_shin1" size="0.049" type="capsule"/>
                            <body name="right_foot" pos="-0.02 0 -0.42">
                                <joint armature="0.0028" axis="0 -1 0" name="right_ankle" pos="0.02 0 0" range="-20 20" stiffness="10" type="hinge"/>
                                <!-- <geom name="right_foot" type="mesh" mesh="foot_mesh_right" pos="0 0 -0.03" user="0"/> -->
                                <geom name="right_foot" fromto="-.06 .01 .018 .23 -.01 .018" size="0.05" type="box"/>
                                <!-- <site name="foot_r" fromto="-.06 .01 .018 .23 -.01 .018" size="0.05" type="box" rgba="0 0 0 .9"/> -->
                            </body>
                        </body>
                    </body>
                    <body name="left_thigh" pos="0 0.1 -0.04">
                        <joint armature="0.01" axis="-1 0 0" damping="5" name="left_hip_x" pos="0 0 0" range="-25 5" stiffness="10" type="hinge"/>
                        <joint armature="0.01" axis="0 0 -1" damping="5" name="left_hip_z" pos="0 0 0" range="-60 35" stiffness="10" type="hinge"/>
                        <joint armature="0.01" axis="0 1 0" damping="5" name="left_hip_y" pos="0 0 0" range="-110 20" stiffness="20" type="hinge"/>
                        <geom fromto="0 0 0 0 -0.01 -.34" name="left_thigh1" size="0.06" type="capsule"/>
                        <body name="left_shin" pos="0 -0.01 -0.403">
                            <joint armature="0.0060" axis="0 -1 0" name="left_knee" pos="0 0 .02" range="-160 -2" stiffness="10" type="hinge"/>
                            <geom fromto="0 0 0 0 0 -.3" name="left_shin1" size="0.049" type="capsule"/>
                            <body name="left_foot" pos="-0.02 0 -0.42">
                                <joint armature="0.0028" axis="0 -1 0" name="left_ankle" pos="0.02 0 0" range="-20 20" stiffness="10" type="hinge"/>
                                <!-- <geom name="left_foot" type="mesh" mesh="foot_mesh_left" pos="0 0 -0.03" user="0"/> -->
                                <geom name="left_foot" fromto="-.06 -.01 .018 .23 .01 .018" size="0.05" type="box"/>
                                <!-- <site name="foot_l" fromto="-.06 -.01 .018 .23 .01 .018" size="0.05" type="box" rgba="0 0 0 .9"/> -->
                            </body>
                        </body>
                    </body>
                </body>
            </body>
            <body name="right_upper_arm" pos="0 -0.17 0.06">
                <joint armature="0.0068" axis="2 1 1" name="right_shoulder1" pos="0 0 0" range="-85 60" stiffness="1" type="hinge"/>
                <joint armature="0.0051" axis="0 -1 1" name="right_shoulder2" pos="0 0 0" range="-85 60" stiffness="1" type="hinge"/>
                <joint armature="0.0051" axis="1 -1 -1" name="right_shoulder3" pos="0 0 0" range="-85 85" stiffness="1" type="hinge"/>
                <geom fromto="0 0 0 .16 -.16 -.16" name="right_uarm1" size="0.04 0.16" type="capsule"/>
                <body name="right_lower_arm" pos=".18 -.18 -.18">
                    <joint armature="0.0028" axis="0 -1 1" name="right_elbow" pos="0 0 0" range="-90 50" stiffness="2" type="hinge"/>
                    <geom fromto="0.01 0.01 0.01 .17 .17 .17" name="right_larm" size="0.031" type="capsule"/>
                    <body name="right_hand" pos=".19 .19 .19">
                        <joint armature="0.0028" axis="0 1 -1" name="right_wrist" pos="0 0 0" range="-40 60" stiffness="2" type="hinge"/>
                        <geom name="right_palm" size="0.04" type="sphere"/> 
                        <body name="finger_base_r" pos=".02 .02 .02">
                            <joint armature="0.002" axis="0 1 -1" name="finger_base_joint_r" pos="0 0 0" range="10 80" stiffness="0.01" type="hinge"/>
                            <geom name="finger_base_r1" fromto=".0 .02 -.02 .025 .045 .005" size="0.014" type="capsule"/> 
                            <geom name="finger_base_r2" fromto=".0 .0 .0 .025 .025 .025" size="0.014" type="capsule"/> 
                            <geom name="finger_base_r3" fromto=".0 -.02 .02 .025 .005 .045" size="0.014" type="capsule"/> 
                            <site name="base_site_r" pos=".01 -.005 -.005" size="0.012" rgba="0 0 0 .2"/>
                            <body name="finger_mid_r" pos=".028 .028 .028">
                                <joint armature="0.002" axis="0 1 -1" name="finger_mid_joint_r" pos="0 0 0" range="10 90" stiffness="0.01" type="hinge"/>
                                <geom name="finger_mid_r1" fromto=".0 .02 -.02 .018 .038 -.002" size="0.014" type="capsule"/> 
                                <geom name="finger_mid_r2" fromto=".0 .0 .0 .018 .018 .018" size="0.014" type="capsule"/> 
