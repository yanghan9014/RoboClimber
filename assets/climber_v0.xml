                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>
    <tendon>
        <fixed name="left_hipknee">
            <joint coef="-1" joint="left_hip_y"/>
            <joint coef="1" joint="left_knee"/>
        </fixed>
        <fixed name="right_hipknee">
            <joint coef="-1" joint="right_hip_y"/>
            <joint coef="1" joint="right_knee"/>
        </fixed>
        <spatial name="finger_tendon_r" width="0.005" rgba=".4 .6 .4 1"> 
            <site site="base_site_r"/>
            <site site="mid_site_r"/>
            <site site="top_site_r"/>
            <site site="tip_site_r"/>
        </spatial>
        <spatial name="finger_tendon_l" width="0.005" rgba=".4 .6 .4 1"> 
            <site site="base_site_l"/>
            <site site="mid_site_l"/>
            <site site="top_site_l"/>
            <site site="tip_site_l"/>
        </spatial>
    </tendon>

    <actuator>
        <motor gear="25" joint="right_shoulder1" name="right_shoulder1"/>
        <motor gear="25" joint="right_shoulder2" name="right_shoulder2"/>
        <motor gear="25" joint="right_shoulder3" name="right_shoulder3"/>
        <motor gear="25" joint="left_shoulder1" name="left_shoulder1"/>
        <motor gear="25" joint="left_shoulder2" name="left_shoulder2"/>
        <motor gear="25" joint="left_shoulder3" name="left_shoulder3"/>
        <motor gear="25" joint="right_elbow" name="right_elbow"/>
        <motor gear="25" joint="left_elbow" name="left_elbow"/>
        <motor gear="25" joint="right_wrist" name="right_wrist"/>
        <motor gear="25" joint="left_wrist" name="left_wrist"/>
        <motor gear="100" joint="abdomen_y" name="abdomen_y"/>
        <motor gear="100" joint="abdomen_z" name="abdomen_z"/>
        <motor gear="100" joint="abdomen_x" name="abdomen_x"/>
        <motor gear="100" joint="right_hip_x" name="right_hip_x"/>
        <motor gear="100" joint="right_hip_z" name="right_hip_z"/>
        <motor gear="300" joint="right_hip_y" name="right_hip_y"/>
        <motor gear="100" joint="left_hip_x" name="left_hip_x"/>
        <motor gear="100" joint="left_hip_z" name="left_hip_z"/>
        <motor gear="300" joint="left_hip_y" name="left_hip_y"/>
        <motor gear="200" joint="right_knee" name="right_knee"/>
        <motor gear="200" joint="left_knee" name="left_knee"/>
        <motor gear="30" joint="right_ankle" name="right_ankle"/>
        <motor gear="30" joint="left_ankle" name="left_ankle"/>
        <motor tendon="finger_tendon_r" ctrlrange="-0.4 0.4" gear="2000" name="fingers_r"/>
        <motor tendon="finger_tendon_l" ctrlrange="-0.4 0.4" gear="2000" name="fingers_l"/>
    </actuator>
