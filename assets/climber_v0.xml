                        <body name="finger_base_l" pos=".02 -.02 .02">
                            <joint armature="0.002" axis="0 1 1" name="finger_base_joint_l" pos="0 0 0" range="10 80" stiffness=".01" type="hinge"/>
                            <geom name="finger_base_l1" fromto=".0 -.02 -.02 .025 -.045 .005" size="0.014" type="capsule"/> 
                            <geom name="finger_base_l2" fromto=".0 .0 .0  .025 -.025 .025" size="0.014" type="capsule"/> 
                            <geom name="finger_base_l3" fromto=".0 .02 .02 .025 -.005 .045" size="0.014" type="capsule"/> 
                            <site name="base_site_l" pos=".01 .005 -.005" size="0.012" rgba="0 0 0 .2"/>
                            <body name="finger_mid_l" pos=".028 -.028 .028">
                                <joint armature="0.002" axis="0 1 1" name="finger_mid_joint_l" pos="0 0 0" range="10 90" stiffness=".01" type="hinge"/>
                                <geom name="finger_mid_l1" fromto=".0 -.02 -.02 .018 -.038 -.002" size="0.014" type="capsule"/> 
                                <geom name="finger_mid_l2" fromto=".0 .0 .0 .018 -.018 .018" size="0.014" type="capsule"/> 
                                <geom name="finger_mid_l3" fromto=".0 .02 .02 .018 .002 .038" size="0.014" type="capsule"/> 
                                <site name="mid_site_l" pos=".01 .005 -.005" size="0.012" rgba="0 0 0 .2"/>
                                <body name="finger_tip_l" pos=".02 -.02 .02">
                                    <joint armature="0.002" axis="0 1 1" name="finger_tip_joint_l" pos="0 0 0" range="10 80" stiffness=".01" type="hinge"/>
                                    <geom name="finger_tip_l1" fromto=".0 -.02 -.02 .015 -.035 -.005" size="0.014" type="capsule"/> 
                                    <geom name="finger_tip_l2" fromto=".0 .0 .0 .015 -.015 .015" size="0.014" type="capsule"/> 
                                    <geom name="finger_tip_l3" fromto=".0 .02 .02 .015 .005 .035" size="0.014" type="capsule"/> 
                                    <site name="top_site_l" pos=".01 .005 -.005" size="0.012" rgba="0 0 0 .2"/>
